<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.ecomaxapp" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ecomax</name>
    <description>
        Aplicativo da Ecomax.
    </description>
    <author email="joao@velty.com.br" href="https://www.velty.com.br/">
        Velty
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
